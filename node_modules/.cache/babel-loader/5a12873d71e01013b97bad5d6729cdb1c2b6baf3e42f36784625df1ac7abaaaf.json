{"ast":null,"code":"import Breadcrumb from './components/Breadcrumb.vue';\nimport PlaceHolder from './components/PlaceHolder.vue';\nexport default {\n  components: {\n    Breadcrumb,\n    PlaceHolder\n  },\n  data() {\n    return {\n      isGameSelected: false,\n      games: this.$store.state.games,\n      game: '',\n      files: Array,\n      file: Object\n    };\n  },\n  methods: {\n    setGame(game) {\n      this.isGameSelected = true;\n      this.game = game;\n      this.files = game.files;\n    },\n    setFile(file) {\n      this.file = file;\n    },\n    inject() {\n      const game = document.getElementsByClassName('game');\n      for (let i = 0; i < game.length; i++) {\n        if (game[i].textContent.toLowerCase() == this.$route.params.gameUrl) {\n          game[i].click();\n        }\n      }\n    }\n  },\n  mounted() {\n    let injectInterval = setInterval(() => {\n      if (document.getElementsByClassName('placeholder')[0]) {\n        clearInterval(injectInterval);\n        this.inject();\n      }\n    }, 10);\n  }\n};","map":{"version":3,"names":["Breadcrumb","PlaceHolder","components","data","isGameSelected","games","$store","state","game","files","Array","file","Object","methods","setGame","setFile","inject","document","getElementsByClassName","i","length","textContent","toLowerCase","$route","params","gameUrl","click","mounted","injectInterval","setInterval","clearInterval"],"sources":["D:\\Sina\\Web-PJs\\_0_o_ForTestingPurposes\\ReX\\rex\\src\\App.vue"],"sourcesContent":["<template>\n  <PlaceHolder />\n  <header class=\"fixed top-0 inset-x-0 bg-black z-10\">\n    <nav>\n      <ul class=\"flex items-center flex-nowrap overflow-auto gap-6 px-6 h-16\">\n        <li class=\"h-full items-center not-last-child:after:content-c not-last-child:after:ml-6 not-last-child:after:text-white\" v-for=\"game in games\" :key=\"game.id\" :class=\"{ 'flex': !isGameSelected, 'hidden': isGameSelected }\">\n          <router-link class=\"text-c-blue hover:text-c-yellow transition-colors underline whitespace-nowrap flex items-center h-full game\" :to=\"{ name: 'app', params: { gameUrl: game.url } }\" @click=\"setGame(game)\">\n            {{ game.title }}\n          </router-link>\n        </li>\n        <li class=\"h-full items-center not-last-child:after:content-c not-last-child:after:ml-6 not-last-child:after:text-white\" v-for=\"file in files\" :key=\"file.id\" :class=\"{ 'hidden': !isGameSelected, 'flex': isGameSelected }\">\n          <router-link class=\"text-c-blue hover:text-c-yellow transition-colors underline whitespace-nowrap flex items-center h-full file\" :to=\"{ name: 'file', params: { gameUrl: game.url, fileUrl: file.url } }\" @click=\"setFile(file)\">\n            {{ file.title }}\n          </router-link>\n        </li>\n      </ul>\n    </nav>\n  </header>\n  <Breadcrumb :file=\"file\" :game=\"game\" />\n  <main class=\"max-w-c mx-auto\">\n    <router-view :file=\"file\"></router-view>\n  </main>\n</template>\n\n<script>\nimport Breadcrumb from './components/Breadcrumb.vue'\nimport PlaceHolder from './components/PlaceHolder.vue'\nexport default {\n  components: {\n    Breadcrumb,\n    PlaceHolder\n  },\n  data(){\n    return{\n      isGameSelected: false,\n      games: this.$store.state.games,\n      game: '',\n      files: Array,\n      file: Object,\n    }\n  },\n  methods: {\n    setGame(game){\n      this.isGameSelected= true\n      this.game= game\n      this.files= game.files\n    },\n    setFile(file){\n      this.file= file\n    },\n    inject(){\n      const game= document.getElementsByClassName('game')\n      for(let i=0; i<game.length; i++){\n        if(game[i].textContent.toLowerCase() == this.$route.params.gameUrl){\n          game[i].click()\n        }\n      }\n    }\n  },\n  mounted(){\n    let injectInterval= setInterval(() => {\n      if(document.getElementsByClassName('placeholder')[0]){\n        clearInterval(injectInterval)\n        this.inject()\n      }\n    }, 10)\n  }\n}\n</script>"],"mappi