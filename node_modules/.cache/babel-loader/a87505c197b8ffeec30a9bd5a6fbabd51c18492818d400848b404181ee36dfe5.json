{"ast":null,"code":"import Breadcrumb from './components/Breadcrumb.vue';\nimport PlaceHolder from './components/PlaceHolder.vue';\nexport default {\n  components: {\n    Breadcrumb,\n    PlaceHolder\n  },\n  data() {\n    return {\n      isGameSelected: false,\n      games: this.$store.state.games,\n      game: '',\n      files: Array,\n      file: Object,\n      isRead: false\n    };\n  },\n  methods: {\n    setGame(game) {\n      this.isGameSelected = true;\n      this.game = game;\n      this.files = game.files;\n    },\n    setFile(file) {\n      this.file = file;\n    },\n    inject() {\n      const game = document.getElementsByClassName('game');\n      for (let i = 0; i < game.length; i++) {\n        if (game[i].textContent.toLowerCase() == this.$route.params.gameUrl) {\n          game[i].click();\n        }\n      }\n      const file = document.getElementsByClassName('file');\n      for (let i = 0; i < file.length; i++) {\n        if (file[i].textContent.toLowerCase().replaceAll(' ', '-').replaceAll('\\'', '-') == this.$route.params.fileUrl) {\n          file[i].click();\n        }\n      }\n    }\n  },\n  mounted() {\n    let injectInterval = setInterval(() => {\n      if (!this.isRead) {\n        this.isRead = true;\n      }\n      if (document.getElementsByClassName('placeholder')[0]) {\n        clearInterval(injectInterval);\n        this.inject();\n      }\n    }, 10);\n  }\n};","map":{"version":3,"names":["Breadcrumb","PlaceHolder","components","data","isGameSelected","games","$store","state","game","files","Array","file","Object","isRead","methods","setGame","setFile","inject","document","getElementsByClassName","i","length","textContent","toLowerCase","$route","params","gameUrl","click","replaceAll","fileUrl","mounted","injectInterval","setInterval","clearInterval"],"sources":["D:\\Sina\\Web-PJs\\_0_o_ForTestingPurposes\\ReX\\rex\\src\\App.vue"],"sourcesContent":["<template>\n  <PlaceHolder />\n  <header class=\"fixed top-0 inset-x-0 bg-black z-10\">