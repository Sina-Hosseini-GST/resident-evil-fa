me('game')\n      for(let i=0; i<game.length; i++){\n        if(game[i].textContent.toLowerCase() == this.$route.params.gameUrl){\n          game[i].click()\n        }\n      }\n      const file= document.getElementsByClassName('file')\n      for(let i=0; i<file.length; i++){\n        if(file[i].textContent.toLowerCase().replaceAll(' ', '-').replaceAll('\\'', '-') == this.$route.params.fileUrl){\n          file[i].click()\n        }\n      }\n    }\n  },\n  mounted(){\n    let injectInterval= setInterval(() => {\n      if(!this.isRead){\n        this.isRead= true\n        \n      }\n      if(document.getElementsByClassName('placeholder')[0]){\n        clearInterval(injectInterval)\n        this.inject()\n      }\n    }, 10)\n  }\n}\n</script>"],"mappings":"AAyBA,OAAOA,UAAS,MAAO,6BAA4B;AACnD,OAAOC,WAAU,MAAO,8BAA6B;AACrD,eAAe;EACbC,UAAU,EAAE;IACVF,UAAU;IACVC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,cAAc,EAAE,KAAK;MACrBC,KAAK,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,KAAK;MAC9BG,IAAI,EAAE,EAAE;MACRC,KAAK,EAAEC,KAAK;MACZC,IAAI,EAAEC,MAAM;MACZC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,OAAOA,CAACP,IAAI,EAAC;MACX,IAAI,CAACJ,cAAc,GAAE,IAAG;MACxB,IAAI,CAACI,IAAI,GAAEA,IAAG;MACd,IAAI,CAACC,KAAK,GAAED,IAAI,CAACC,KAAI;IACvB,CAAC;IACDO,OAAOA,CAACL,IAAI,EAAC;MACX,IAAI,CAACA,IAAI,GAAEA,IAAG;IAChB,CAAC;IACDM,MAAMA,CAAA,EAAE;MACN,MAAMT,IAAI,GAAEU,QAAQ,CAACC,sBAAsB,CAAC,MAAM;MAClD,KAAI,IAAIC,CAAC,GAAC,CAAC,EAAEA,CAAC,GAACZ,IAAI,CAACa,MAAM,EAAED,CAAC,EAAE,EAAC;QAC9B,IAAGZ,IAAI,CAACY,CAAC,CAAC,CAACE,WAAW,CAACC,WAAW,CAAC,KAAK,IAAI,CAACC,MAAM,CAACC,MAAM,CAACC,OAAO,EAAC;UACjElB,IAAI,CAACY,CAAC,CAAC,CAACO,KAAK,CAAC;QAChB;MACF;MACA,MAAMhB,IAAI,GAAEO,QAAQ,CAACC,sBAAsB,CAAC,MAAM;MAClD,KAAI,IAAIC,CAAC,GAAC,CAAC,EAAEA,CAAC,GAACT,IAAI,CAACU,MAAM,EAAED,CAAC,EAAE,EAAC;QAC9B,IAAGT,IAAI,CAACS,CAAC,CAAC,CAACE,WAAW,CAACC,WAAW,CAAC,CAAC,CAACK,UAAU,CAAC,GAAG,EAAE,GAAG,CAAC,CAACA,UAAU,CAAC,IAAI,EAAE,GAAG,KAAK,IAAI,CAACJ,MAAM,CAACC,MAAM,CAACI,OAAO,EAAC;UAC5GlB,IAAI,CAACS,CAAC,CAAC,CAACO,KAAK,CAAC